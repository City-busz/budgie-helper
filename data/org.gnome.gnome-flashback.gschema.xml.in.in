<schemalist gettext-domain="@GETTEXT_PACKAGE@">
	<schema id="org.gnome.gnome-flashback" path="/org/gnome/gnome-flashback/">
		<key name="desktop-background" type="b">
			<default>true</default>
			<_summary>Desktop background</_summary>
			<_description>If set to true, then GNOME Flashback application will be used to draw desktop background.</_description>
		</key>
		<key name="display-config" type="b">
			<default>true</default>
			<_summary>Display config</_summary>
			<_description>If set to true, then GNOME Flashback application will be used to provide display configuration.</_description>
		</key>
		<key name="end-session-dialog" type="b">
			<default>true</default>
			<_summary>End session dialog</_summary>
			<_description>If set to true, then GNOME Flashback application will be used to show end session dialog.</_description>
		</key>

		<!--
		<child name="background" schema="org.gnome.gnome-flashback.background"/>
		-->
	</schema>

	<!--
	<schema id="org.gnome.gnome-flashback.background" path="/org/gnome/gnome-flashback/background/">
		<key name="fade" type="b">
			<default>true</default>
			<_summary>Fade the background on change</_summary>
			<_description>If set to true, then fade effect will be used to change the desktop background.</_description>
		</key>
	</schema>
	-->
</schemalist>
