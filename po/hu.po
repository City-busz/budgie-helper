#, c-format
msgid "%s (%s)"
msgstr "%s (%s)"

#: ../gnome-flashback/libpolkit/flashback-polkit-dialog.c:494
msgid "Action:"
msgstr "Művelet:"

#: ../gnome-flashback/libpolkit/flashback-polkit-dialog.c:497
